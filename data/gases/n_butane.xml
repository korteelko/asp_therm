    <group name="constants">
      <subgroup name="critical_point">
        <parameter name="volume"> 0.0 </parameter>
        <parameter name="pressure"> 3800000 </parameter>
        <parameter name="temperature"> 425.0 </parameter>
        <parameter name="compress_factor"> 0.274 </parameter>
      </subgroup>
      <subgroup name="const_parameters">
        <parameter name="molec_mass"> 58.123 </parameter>
        <parameter name="acentric"> 0.193 </parameter>
      </subgroup>
    </group>
    <group name="dynamics">
      <subgroup name="point">
        <!-- Если оставить пустым то словим ошибку  -->
        <parameter name="volume"> 1.42 </parameter>
        <parameter name="pressure"> 100000 </parameter>
