      </subgroup>
      <subgroup name="capacities">
        <parameter name="heat_cap_vol"> </parameter>
        <parameter name="heat_cap_pres"> 1580 </parameter>
      </subgroup>
      <subgroup name="potentials">
        <parameter name="internal_energy"> </parameter>
      </subgroup>
    </group>
  </section>
  <section name="data">
    <group name="chemestry">
      <subgroup name="data">
        <parameter name="formula"> C4H10 </parameter>
        <parameter name="image_url"> !!! </parameter>
      </subgroup>
    </group>
  </section>
