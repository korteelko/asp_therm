        <parameter name="heat_cap_vol"> 1660 </parameter>
        <parameter name="heat_cap_pres"> 2185 </parameter>
      </subgroup>
    </group>
  </section>
  <section name="data">
    <group name="chemestry">
      <subgroup name="data">
        <parameter name="formula"> CH4 </parameter>
        <parameter name="image_url"> Methane-2D-dimensions.svg </parameter>
      </subgroup>
    </group>
  </section>
</gas>
