<?xml version="1.0" encoding="UTF-8"?>
<gas name="methane" version="1.0">
  <section name="parameters">
    <!-- [v]=m3/kg [p]=MPa [t]=K -->
    <group name="constants">
      <subgroup name="critical_point">
        <parameter name="volume"> 0.0061728395 </parameter>
        <parameter name="pressure"> 4.226 </parameter>
        <parameter name="temperature"> 190.66 </parameter>
      </subgroup>
      <subgroup name="const_parameters">
        <parameter name="molec_mass"> 16.043 </parameter>
        <parameter name="acentric"> 0.011 </parameter>
      </subgroup>
    </group>
    <group name="dynamics">
      <subgroup name="point">
        <parameter name="volume"> 1.42 </parameter>        
        <parameter name="pressure"> 0.1 </parameter>
        <parameter name="temperature"> 275 </parameter>
      </subgroup>
      <subgroup name="capacities">
        <!-- cp by Wiki, cv by Mayer law -->
